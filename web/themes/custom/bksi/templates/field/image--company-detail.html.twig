{#
/**
 * @file
 * Theme override of an image.
 *
 * Available variables:
 * - attributes: HTML attributes for the img tag.
 * - style_name: (optional) The name of the image style applied.
 *
 * @see template_preprocess_image()
 */
#}
<picture>
  <source srcset="{{ file_url( uri| image_style('large_webp')) }}" media="only screen and (max-width: 450px)" type="image/webp" >
  <source srcset="{{ file_url( uri| image_style('original_webp')) }}" media="only screen and (min-width: 450px)" type="image/webp">
  <img class="fade-in-image object-cover h-full w-full" src="{{ attributes.src }}" alt="{{ attributes.alt }}" />
</picture>
